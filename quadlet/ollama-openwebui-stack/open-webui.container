[Unit]
Description=Open-WebUI container

[Container]
ContainerName=openwebui.container
Image=ghcr.io/open-webui/open-webui:latest
Pod=ollama.pod

Timezone=local

EnvironmentFile=./%n.d/env

Volume=openwebui:/app/backend/data:Z

LogDriver=journald
LogOpt=tag=openwebui
Notify=container

AutoUpdate=registry

Label=role=webui
Label=app=ai

[Service]
Restart=always
TimeoutStartSec=120

[Install]
WantedBy=multi-user.target default.target
